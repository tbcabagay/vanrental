{
    "name": "kartik-v/yii2-app-practical-b",
	"description": "Yii 2 Practical-B Application Template",
	"keywords": ["yii2", "framework", "practical", "practical-b", "basic", "application template"],
	"homepage": "http://demos.krajee.com/app-practical-b",
	"type": "project",
	"license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Kartik Visweswaran",
            "email": "kartikv2@gmail.com",
            "homepage": "http://www.krajee.com/"
        }
    ],
	"support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.5",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "bower-asset/font-awesome": "~4.7.0",
        "bower-asset/scrollreveal": "~3.3.2",
        "bower-asset/magnific-popup": "~1.1.0",
        "bower-asset/bootbox": "~4.4.0",
        "kartik-v/yii2-helpers": "dev-master",
        "kartik-v/yii2-grid": "@dev",
        "kartik-v/yii2-widgets": "*",
        "kartik-v/yii2-markdown": "dev-master",
        "yiisoft/yii2-jui": "~2.0.0",
        "yiisoft/yii2-authclient": "~2.1.0"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ],
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}